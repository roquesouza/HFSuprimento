enderecodao.pesquisarporpaciente=SELECT E.ID,E.LOGRADOURO_ID,L.DESCRICAO,L.CEP,L.TIPO_LOGRADOURO_ID,(SELECT T.DESCRICAO FROM TIPO_LOGRADOURO T WHERE T.ID = L.TIPO_LOGRADOURO_ID),B.ID,B.DESCRICAO,C.ID,C.DESCRICAO,ES.ID,ES.SIGLA,E.NUMERO,E.COMPLEMENTO,E.FLAG_ATIVO FROM ENDERECO E,LOGRADOURO L,BAIRRO B,CIDADE C,ESTADO ES WHERE L.ID = E.LOGRADOURO_ID AND L.BAIRRO_ID = B.ID AND B.CIDADE_ID = C.ID AND C.ESTADO_ID = ES.ID AND E.PACIENTE_ID = ?
enderecodao.pesquisarativoporpaciente=SELECT E.ID,E.LOGRADOURO_ID,L.DESCRICAO,L.CEP,B.ID,B.DESCRICAO,C.ID,C.DESCRICAO,ES.ID,ES.SIGLA,E.NUMERO,E.COMPLEMENTO,E.FLAG_ATIVO FROM ENDERECO E,LOGRADOURO L,BAIRRO B,CIDADE C,ESTADO ES WHERE L.ID = E.LOGRADOURO_ID AND L.BAIRRO_ID = B.ID AND B.CIDADE_ID = C.ID AND C.ESTADO_ID = ES.ID AND E.PACIENTE_ID = ? AND FLAG_ATIVO IS TRUE
enderecodao.obter=SELECT E.ID,E.LOGRADOURO_ID,L.DESCRICAO,L.CEP,B.ID,B.DESCRICAO,C.ID,C.DESCRICAO,ES.ID,ES.SIGLA,E.NUMERO,E.COMPLEMENTO,E.FLAG_ATIVO FROM ENDERECO E,LOGRADOURO L,BAIRRO B,CIDADE C,ESTADO ES WHERE L.ID = E.LOGRADOURO_ID AND L.BAIRRO_ID = B.ID AND B.CIDADE_ID = C.ID AND C.ESTADO_ID = ES.ID AND E.PACIENTE_ID = ? AND FLAG_ATIVO IS TRUE LIMIT 1
