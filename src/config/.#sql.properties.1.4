logradourodao.pesquisarporbairro=SELECT ID,TIPO_LOGRADOURO_ID,(SELECT T.DESCRICAO FROM TIPO_LOGRADOURO T WHERE T.ID = TIPO_LOGRADOURO_ID),DESCRICAO FROM LOGRADOURO WHERE BAIRRO_ID = ?
logradourodao.obter=SELECT L.ID,L.TIPO_LOGRADOURO_ID,(SELECT T.DESCRICAO FROM TIPO_LOGRADOURO T WHERE T.ID = L.TIPO_LOGRADOURO_ID),L.DESCRICAO,L.CEP,L.BAIRRO_ID,(SELECT B.DESCRICAO FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID),(SELECT B.CIDADE_ID FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID),(SELECT C.DESCRICAO FROM CIDADE C WHERE C.ID = (SELECT B.CIDADE_ID FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID)),(SELECT C.ESTADO_ID FROM CIDADE C WHERE C.ID = (SELECT B.CIDADE_ID FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID)),(SELECT E.SIGLA FROM ESTADO E WHERE E.ID = (SELECT C.ESTADO_ID FROM CIDADE C WHERE C.ID = (SELECT B.CIDADE_ID FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID))),L.USUARIO_CADASTRO_ID,(SELECT U.NOME FROM USUARIO U WHERE U.ID = L.USUARIO_CADASTRO_ID),L.DATA_CADASTRO FROM LOGRADOURO L WHERE L.ID = ?
logradourodao.obterporcep=SELECT L.ID,L.TIPO_LOGRADOURO_ID,(SELECT T.DESCRICAO FROM TIPO_LOGRADOURO T WHERE T.ID = L.TIPO_LOGRADOURO_ID),L.DESCRICAO,L.CEP,L.BAIRRO_ID,(SELECT B.DESCRICAO FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID),(SELECT B.CIDADE_ID FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID),(SELECT C.DESCRICAO FROM CIDADE C WHERE C.ID = (SELECT B.CIDADE_ID FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID)),(SELECT C.ESTADO_ID FROM CIDADE C WHERE C.ID = (SELECT B.CIDADE_ID FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID)),(SELECT E.SIGLA FROM ESTADO E WHERE E.ID = (SELECT C.ESTADO_ID FROM CIDADE C WHERE C.ID = (SELECT B.CIDADE_ID FROM BAIRRO B WHERE B.ID = L.BAIRRO_ID))),L.USUARIO_CADASTRO_ID,(SELECT U.NOME FROM USUARIO U WHERE U.ID = L.USUARIO_CADASTRO_ID),L.DATA_CADASTRO FROM LOGRADOURO L WHERE L.CEP = ?
logradourodao.alterar=UPDATE LOGRADOURO SET TIPO_LOGRADOURO_ID = ?,DESCRICAO = ?,CEP = ?,BAIRRO_ID = ? WHERE ID = ?
