menudao.pesquisarmenuraiz=SELECT DISTINCT M.ID, M.DESCRICAO, M.URL, M.TAB_ATIVA, M.MENU_ID, M.ORDEM FROM MENU M, MENU M2 WHERE M.ID = M2.MENU_ID AND M.FLAG_ATIVO = TRUE AND M.MENU_ID IS NULL AND M2.ID IN (SELECT DISTINCT M.ID FROM MENU M, MENU_FUNCAO_ORIGEM MFO, FUNCAO_ORIGEM FO, USUARIO_FUNCAO UF WHERE M.ID = MFO.MENU_ID AND MFO.FUNCAO_ORIGEM_ID = FO.ID AND FO.FUNCAO_ID = UF.FUNCAO_ID AND FO.ORIGEM_ID = UF.ORIGEM_ID AND M.FLAG_ATIVO = TRUE AND M.MENU_ID IS NOT NULL AND UF.USUARIO_ID = ? AND FO.ORIGEM_ID = ?) ORDER BY M.ORDEM
